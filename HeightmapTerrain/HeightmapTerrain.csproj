<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
  </PropertyGroup>
  <ItemGroup>
    <PackageReference Include="Xenko.Engine" Version="3.1.0.1-beta02-0752" />

    <PackageReference Include="Xenko.Video" Version="3.1.0.1-beta02-0752" />
    <PackageReference Include="Xenko.Physics" Version="3.1.0.1-beta02-0752" />
    <PackageReference Include="Xenko.Navigation" Version="3.1.0.1-beta02-0752" />
    <PackageReference Include="Xenko.Particles" Version="3.1.0.1-beta02-0752" />
    <PackageReference Include="Xenko.UI" Version="3.1.0.1-beta02-0752" />
  </ItemGroup>
  <ItemGroup>
    <Compile Update="Effects\MyCustomShader.cs">
      <DesignTime>True</DesignTime>
      <DesignTimeSharedInput>True</DesignTimeSharedInput>
      <AutoGen>True</AutoGen>
      <DependentUpon>MyCustomShader.xksl</DependentUpon>
    </Compile>
  </ItemGroup>
  <ItemGroup>
    <None Update="Effects\MyCustomShader.xksl">
      <Generator>XenkoShaderKeyGenerator</Generator>
      <LastGenOutput>MyCustomShader.cs</LastGenOutput>
    </None>
  </ItemGroup>
</Project>